digraph CoomassieBlueStainProtocol {
    // Define the title
    Title [label="\"Coomassie Blue Stain\""];
    
    // Define steps
    Step1 [label="1. Repeat steps 1-8 of the Western Blot protocol."];
    Step2 [label="2. Once gels have finished running, cover gel with Coomassie Brilliant Blue, cover with lid, and incubate for 30 min."];
    Step3 [label="3. Rinse with water twice."];
    Step4 [label="4. Add enough destain solution to cover the gel and leave overnight, gently shaking at room temperature."];
    
    // Define edges (sequence of steps)
    Title -> Step1;
    Step1 -> Step2;
    Step2 -> Step3;
    Step3 -> Step4;
}
